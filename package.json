{
  "name": "isthepatchout",
  "version": "0.0.0",
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint src",
    "postinstall": "simple-git-hooks",
    "serve": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "1.9.0",
    "modern-normalize": "1.0.0",
    "vue": "3.0.11"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.1.3",
    "@vitejs/plugin-vue": "1.2.1",
    "@vue/compiler-sfc": "3.0.11",
    "eslint": "7.23.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "simple-git-hooks": "2.3.1",
    "typescript": "4.2.4",
    "vite": "2.1.5",
    "vue-tsc": "0.0.21"
  }
}
