{
  "name": "isthepatchout",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "homepage": "https://isthepatchout.com",
  "repository": "https://github.com/BeeeQueue/isthepatchout",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.0.0",
  "engines": {
    "node": "^14.17"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite --port $PORT --clearScreen false",
    "build": "vite build",
    "bundlewatch": "bundlewatch --config .bundlewatchrc",
    "lint": "eslint {api,src}",
    "postinstall": "simple-git-hooks",
    "serve": "vite preview",
    "start": "vite",
    "typecheck": "tsc --project api/tsconfig.json && vue-tsc --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "14.17.26"
  },
  "dependencies": {
    "@ivanv/vue-collapse-transition": "1.0.2",
    "@sentry/vue": "6.11.0",
    "@supabase/supabase-js": "1.24.0",
    "@vueuse/core": "6.5.3",
    "date-fns": "2.25.0",
    "fathom-client": "3.2.0",
    "simple-icons": "5.18.0",
    "vue": "3.2.20",
    "workbox-precaching": "6.3.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.2.1",
    "@hapi/boom": "9.1.4",
    "@sentry/node": "6.11.0",
    "@sentry/tracing": "6.11.0",
    "@types/node": "14.17.26",
    "@types/pino": "6.3.11",
    "@types/simple-icons": "5.8.0",
    "@vercel/node": "1.12.1",
    "@vitejs/plugin-vue": "1.9.3",
    "@vue/compiler-sfc": "3.2.20",
    "@windicss/animations": "1.0.8",
    "bottleneck": "2.19.5",
    "browserslist": "4.17.4",
    "bundlewatch": "0.3.2",
    "dotenv": "10.0.0",
    "esbuild-plugin-browserslist": "0.2.1",
    "eslint": "8.1.0",
    "got": "11.8.2",
    "joi": "17.4.2",
    "lint-staged": "11.1.2",
    "pino": "6.13.3",
    "pino-pretty": "5.1.3",
    "prettier": "2.4.1",
    "simple-git-hooks": "2.6.1",
    "tailwindcss": "2.2.17",
    "typescript": "4.3.5",
    "vite": "2.6.7",
    "vite-plugin-pwa": "0.11.3",
    "vite-plugin-windicss": "1.4.11",
    "vue-tsc": "0.28.3",
    "windicss": "3.1.9"
  }
}
