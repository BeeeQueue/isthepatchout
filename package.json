    "@vueuse/nuxt": "9.13.0",
    "@windicss/plugin-animations": "1.0.9",
    "browserslist": "4.21.5",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "lint-staged": "13.1.2",
    "myzod": "1.10.0",
    "nuxt": "3.2.2",
    "nuxt-icon": "0.3.2",
    "nuxt-security": "0.10.2",
    "nuxt-windicss": "2.6.0",
    "ofetch": "1.0.1",
    "pino-pretty": "9.4.0",
    "prettier": "2.8.4",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vue": "3.2.47",
    "vue-tsc": "1.2.0"
  }
