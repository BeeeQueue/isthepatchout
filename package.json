{
  "name": "isthepatchout",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "homepage": "https://isthepatchout.com",
  "repository": "https://github.com/BeeeQueue/isthepatchout",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.2",
  "engines": {
    "node": ">=16.15"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite --port $PORT --clearScreen false",
    "build": "vite build",
    "bundlewatch": "bundlewatch --config .bundlewatchrc",
    "lint": "eslint {api,src}",
    "postinstall": "simple-git-hooks",
    "serve": "vite preview",
    "start": "vite",
    "typecheck": "tsc --project api/tsconfig.json && vue-tsc --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "16.11.45",
    "node-fetch": "npm:node-fetch-native@0.1.4"
  },
  "dependencies": {
    "@ivanv/vue-collapse-transition": "1.0.2",
    "@supabase/supabase-js": "1.35.4",
    "@vueuse/core": "8.9.4",
    "canvas-confetti": "1.5.1",
    "date-fns": "2.29.1",
    "dotaver": "1.0.1",
    "fathom-client": "3.5.0",
    "simple-icons": "7.5.0",
    "vue": "3.2.37",
    "workbox-precaching": "6.5.3"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.6.0",
    "@hapi/boom": "10.0.0",
    "@sentry/node": "7.7.0",
    "@types/canvas-confetti": "1.4.3",
    "@types/node": "16.11.45",
    "@vercel/node": "2.4.5",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/compiler-sfc": "3.2.37",
    "@windicss/animations": "1.0.8",
    "bottleneck": "2.19.5",
    "browserslist": "4.21.3",
    "bundlewatch": "0.3.3",
    "dotenv": "16.0.1",
    "esbuild-plugin-browserslist": "0.4.12",
    "eslint": "8.20.0",
    "got": "11.8.5",
    "joi": "17.6.0",
    "lint-staged": "13.0.3",
    "pino": "8.3.1",
    "pino-pretty": "8.1.0",
    "prettier": "2.7.1",
    "rollup-plugin-visualizer": "5.7.1",
    "simple-git-hooks": "2.8.0",
    "tailwindcss": "3.1.6",
    "typescript": "4.7.4",
    "vite": "2.9.14",
    "vite-plugin-checker": "0.4.9",
    "vite-plugin-pwa": "0.12.3",
    "vite-plugin-windicss": "1.8.7",
    "vue-tsc": "0.39.0",
    "windicss": "3.5.6"
  }
}
