    "pino": "8.10.0",
    "remeda": "1.6.2"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "1.1.2",
    "@nuxt/devtools": "0.1.4",
    "@nuxtjs/fontaine": "0.2.4",
    "@nuxtjs/google-fonts": "3.0.0-1",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/supabase": "0.3.1",
