{
  "name": "isthepatchout",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "homepage": "https://isthepatchout.com",
  "repository": "https://github.com/BeeeQueue/isthepatchout",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": "^16.15"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite --port $PORT --clearScreen false",
    "build": "vite build",
    "bundlewatch": "bundlewatch --config .bundlewatchrc",
    "lint": "eslint {api,src}",
    "postinstall": "simple-git-hooks",
    "serve": "vite preview",
    "start": "vite",
    "typecheck": "tsc --project api/tsconfig.json && vue-tsc --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "16.11.65",
    "node-fetch": "npm:node-fetch-native@0.1.7"
  },
  "dependencies": {
    "@ivanv/vue-collapse-transition": "1.0.2",
    "@supabase/supabase-js": "2.0.0-rc.9",
    "@vueuse/core": "9.3.0",
    "canvas-confetti": "1.5.1",
    "date-fns": "2.29.3",
    "dotaver": "1.0.1",
    "fathom-client": "3.5.0",
    "simple-icons": "7.15.0",
    "vue": "3.2.40",
    "workbox-precaching": "6.5.4"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.8.0",
    "@hapi/boom": "10.0.0",
    "@sentry/node": "7.15.0",
    "@types/canvas-confetti": "1.4.3",
    "@types/node": "16.11.65",
    "@vercel/node": "2.5.24",
    "@vitejs/plugin-vue": "3.1.2",
    "@vue/compiler-sfc": "3.2.40",
    "@windicss/animations": "1.0.8",
    "browserslist": "4.21.4",
    "bundlewatch": "0.3.3",
    "dotenv": "16.0.3",
    "esbuild-plugin-browserslist": "0.5.1",
    "eslint": "8.25.0",
    "lint-staged": "13.0.3",
    "myzod": "1.8.8",
    "ohmyfetch": "0.4.19",
    "p-queue": "6.6.2",
    "pino": "8.6.1",
    "pino-pretty": "9.1.1",
    "prettier": "2.7.1",
    "remeda": "1.1.0",
    "rollup-plugin-visualizer": "5.8.2",
    "simple-git-hooks": "2.8.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-plugin-checker": "0.4.9",
    "vite-plugin-pwa": "0.13.1",
    "vite-plugin-windicss": "1.8.8",
    "vue-tsc": "0.40.13",
    "windicss": "3.5.6"
  }
}
