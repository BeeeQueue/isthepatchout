    "date-fns": "2.22.1",
    "fathom-client": "3.0.0",
    "vue": "3.1.1",
    "workbox-precaching": "6.1.5"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.2.1",
    "@hapi/boom": "9.1.2",
    "@sentry/node": "6.6.0",
    "@sentry/tracing": "6.6.0",
    "@types/node": "14.17.3",
    "@types/pino": "6.3.8",
    "@types/web-push": "3.3.0",
    "@vercel/node": "1.11.0",
    "@vitejs/plugin-vue": "1.2.3",
    "@vue/compiler-sfc": "3.1.1",
