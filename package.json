{
  "name": "isthepatchout",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "homepage": "https://isthepatchout.com",
  "repository": "https://github.com/BeeeQueue/isthepatchout",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "engines": {
    "node": "^14.17"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite --port $PORT --clearScreen false",
    "build": "vite build",
    "bundlewatch": "bundlewatch --config .bundlewatchrc",
    "lint": "eslint {api,src}",
    "postinstall": "simple-git-hooks",
    "serve": "vite preview",
    "start": "vite",
    "typecheck": "tsc --project api/tsconfig.json && vue-tsc --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "14.18.0"
  },
  "dependencies": {
    "@ivanv/vue-collapse-transition": "1.0.2",
    "@sentry/vue": "6.15.0",
    "@supabase/supabase-js": "1.28.5",
    "@vueuse/core": "7.1.2",
    "date-fns": "2.27.0",
    "fathom-client": "3.2.0",
    "simple-icons": "5.24.0",
    "vue": "3.2.23",
    "workbox-precaching": "6.4.2"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.4.0",
    "@hapi/boom": "9.1.4",
    "@sentry/node": "6.15.0",
    "@types/node": "14.18.0",
    "@types/pino": "7.0.5",
    "@types/simple-icons": "5.21.1",
    "@vercel/node": "1.12.1",
    "@vitejs/plugin-vue": "1.10.1",
    "@vue/compiler-sfc": "3.2.23",
    "@windicss/animations": "1.0.8",
    "bottleneck": "2.19.5",
    "browserslist": "4.18.1",
    "bundlewatch": "0.3.2",
    "dotenv": "10.0.0",
    "esbuild-plugin-browserslist": "0.3.0",
    "eslint": "8.4.0",
    "got": "11.8.3",
    "joi": "17.5.0",
    "lint-staged": "12.1.2",
    "pino": "7.5.1",
    "pino-pretty": "7.2.0",
    "prettier": "2.5.0",
    "simple-git-hooks": "2.7.0",
    "tailwindcss": "2.2.19",
    "typescript": "4.5.2",
    "vite": "2.6.14",
    "vite-plugin-pwa": "0.11.9",
    "vite-plugin-windicss": "1.5.4",
    "vue-tsc": "0.29.8",
    "windicss": "3.2.1"
  }
}
