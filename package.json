    "got": "11.8.2",
    "joi": "17.4.0",
    "lint-staged": "11.0.0",
    "pino": "6.11.3",
    "pino-pretty": "5.0.2",
    "prettier": "2.3.1",
    "simple-git-hooks": "2.4.1",
    "tailwindcss": "2.1.4",
    "typescript": "4.3.2",
    "vite": "2.3.7",
