  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "lint": "eslint {api,src}",
    "postinstall": "simple-git-hooks",
    "serve": "nuxi preview",
    "typecheck": "tsc --project api/tsconfig.json && vue-tsc --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.14.1",
    "node-fetch": "npm:node-fetch-native@1.0.2"
  },
  "dependencies": {
    "@vueuse/core": "9.13.0",
