{
  "name": "isthepatchout",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "homepage": "https://isthepatchout.com",
  "repository": "https://github.com/BeeeQueue/isthepatchout",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.27.1",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite --port $PORT --clearScreen false",
    "build": "vite build",
    "bundlewatch": "bundlewatch --config .bundlewatchrc",
    "lint": "eslint {api,src}",
    "postinstall": "simple-git-hooks",
    "serve": "vite preview",
    "start": "vite",
    "typecheck": "tsc --project api/tsconfig.json && vue-tsc --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.14.0",
    "esbuild": "0.17.9",
    "node-fetch": "npm:node-fetch-native@1.0.2"
  },
  "dependencies": {
    "@ivanv/vue-collapse-transition": "1.0.2",
    "@supabase/realtime-js": "2.6.0",
    "@supabase/supabase-js": "2.8.0",
    "@vueuse/core": "9.13.0",
    "canvas-confetti": "1.6.0",
    "date-fns": "2.29.3",
    "dotaver": "1.0.1",
    "fathom-client": "3.5.0",
    "simple-icons": "8.5.0",
    "vue": "3.2.47",
    "workbox-precaching": "6.5.4"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "1.1.2",
    "@hapi/boom": "10.0.1",
    "@sentry/node": "7.38.0",
    "@types/canvas-confetti": "1.6.0",
    "@types/node": "18.14.0",
    "@vercel/node": "2.9.6",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/compiler-sfc": "3.2.47",
    "@windicss/animations": "1.0.8",
    "browserslist": "4.21.5",
    "bundlewatch": "0.3.3",
    "dotenv": "16.0.3",
    "esbuild-plugin-browserslist": "0.7.0",
    "eslint": "8.34.0",
    "lint-staged": "13.1.2",
    "myzod": "1.8.8",
    "ohmyfetch": "0.4.21",
    "p-queue": "6.6.2",
    "pino": "8.10.0",
    "pino-pretty": "9.3.0",
    "prettier": "2.8.4",
    "remeda": "1.6.2",
    "rollup-plugin-visualizer": "5.9.0",
    "simple-git-hooks": "2.8.1",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vite": "4.1.2",
    "vite-plugin-checker": "0.5.6",
    "vite-plugin-pwa": "0.14.4",
    "vite-plugin-windicss": "1.8.10",
    "vue-tsc": "1.1.4",
    "windicss": "3.5.6"
  }
}
