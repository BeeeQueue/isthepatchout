{
  "name": "isthepatchout",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "homepage": "https://isthepatchout.com",
  "repository": "https://github.com/BeeeQueue/isthepatchout",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": "^14.17"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite --port $PORT --clearScreen false",
    "build": "vite build",
    "bundlewatch": "bundlewatch --config .bundlewatchrc",
    "lint": "eslint {api,src}",
    "postinstall": "simple-git-hooks",
    "serve": "vite preview",
    "start": "vite",
    "typecheck": "tsc --project api/tsconfig.json && vue-tsc --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "14.18.12"
  },
  "dependencies": {
    "@ivanv/vue-collapse-transition": "1.0.2",
    "@supabase/supabase-js": "1.30.6",
    "@vueuse/core": "7.6.2",
    "canvas-confetti": "1.5.1",
    "date-fns": "2.28.0",
    "fathom-client": "3.4.1",
    "simple-icons": "6.11.0",
    "vue": "3.2.31",
    "workbox-precaching": "6.4.2"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.5.0",
    "@hapi/boom": "9.1.4",
    "@sentry/node": "6.17.9",
    "@types/canvas-confetti": "1.4.2",
    "@types/node": "14.18.12",
    "@types/pino": "7.0.5",
    "@types/simple-icons": "5.21.1",
    "@vercel/node": "1.13.0",
    "@vitejs/plugin-vue": "2.2.2",
    "@vue/compiler-sfc": "3.2.31",
    "@windicss/animations": "1.0.8",
    "bottleneck": "2.19.5",
    "browserslist": "4.19.3",
    "bundlewatch": "0.3.3",
    "dotenv": "16.0.0",
    "esbuild-plugin-browserslist": "0.4.3",
    "eslint": "8.10.0",
    "got": "11.8.3",
    "joi": "17.6.0",
    "lint-staged": "12.3.4",
    "pino": "7.8.0",
    "pino-pretty": "7.5.1",
    "prettier": "2.5.1",
    "rollup-plugin-visualizer": "5.5.4",
    "simple-git-hooks": "2.7.0",
    "tailwindcss": "3.0.23",
    "typescript": "4.5.5",
    "vite": "2.8.4",
    "vite-plugin-checker": "0.4.2",
    "vite-plugin-pwa": "0.11.13",
    "vite-plugin-windicss": "1.7.1",
    "vue-tsc": "0.31.4",
    "windicss": "3.4.4"
  }
}
